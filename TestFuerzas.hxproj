  </output>
  <!-- Other classes to be compiled into your SWF -->
  <classpaths>
    <class path="C:\HaxeToolkit\haxe\lib\flixel-addons\1,1,0" />
    <class path="C:\HaxeToolkit\haxe\lib\flixel-ui\1,0,2" />
    <class path="C:\HaxeToolkit\haxe\lib\openfl\3,0,2" />
    <class path="C:\HaxeToolkit\haxe\lib\flixel\3,3,8" />
    <class path="C:\HaxeToolkit\haxe\lib\lime\2,3,2" />
    <class path="C:\HaxeToolkit\haxe\lib\nape\2,0,16" />
    <class path="source" />
    <class path="export\windows\neko\haxe" />
  </classpaths>
  <!-- Build options -->
  <build>
    <option directives="flixel-addons=1.1.0&#xA;flixel-ui=1.0.2&#xA;openfl=3.0.2&#xA;flixel=3.3.8&#xA;lime=2.3.2&#xA;nape=2.0.16&#xA;openfl-next&#xA;tools=2.3.2&#xA;NAPE_NO_INLINE&#xA;FLX_NO_DEBUG&#xA;no-compilation&#xA;NAPE_RELEASE_BUILD&#xA;native&#xA;lime-native&#xA;openfl-native&#xA;FLX_RENDER_TILE&#xA;windows&#xA;desktop" />
